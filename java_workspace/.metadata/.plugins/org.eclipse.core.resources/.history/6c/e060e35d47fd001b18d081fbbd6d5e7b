	switch(select) 
	{
	case 1:
	 System.out.println("i bet your entry will be printed twice");
     String entry=scanner.nextLine();
     System.out.println();
     printTwice(entry);
     scanner.close();
	case 2:
		String index[]= {"number","number","number","number"};
	
		for(int i=0;i<4;i++) {
	System.out.println("enter "+index[i]);
	 int a = scanner.nextInt();
		
	//math(a,a,a,a);
	}
	}
	}	
	
public static void printTwice(String s) {
System.out.println(s);
System.out.println(s);
}
public static void math(int x,int y,int z,int q) {
	System.out.println(x+y+z+q);
	
}























